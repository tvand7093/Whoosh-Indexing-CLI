{
	"editor.useTabStops": true,
	"editor.insertSpaces": false,
	"python.linting.pylintEnabled": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true,
		"**/mysql-data":true,
		"**/*.pyc":{ "when": "$(basename).py" },
		"**/*.pye":{ "when": "$(basename).py" },
		"**/*~": true
	}
}